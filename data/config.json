{
  "project": {
    "name": "EvoIntTracker",
    "quality": 1
 },
  "loader": {
    "info": "data/info.md",
    "timeline": "data/timeline.csv",
    "items": "data/data.csv",
    "textures": {
      "medium": {
        "size": 256,
        "url": "data/images/sprites/spritesheet.json"
      },
      "detail": {
        "size": 1024,
        "url": "data/images/1024/"
      },
      "big": {
        "size": 4096,
        "url": "data/images/4096/"
      }
    }
  },
  "style": {
    "fontColor": "#09BE79",
    "fontColorActive": "#eae2cb",
    "fontBackground": "#044E31",
    "textShadow": "1px 1px 0px #044E31",
    "canvasBackground": "#2B2B2B",
    "timelineBackground": "#0AD688",
    "timelineFontColor": "#000",
    "backgroundHeader": "#035151",
    "fontColorHeader": "#f8774f",
    "detailBackground": "#CCC",
    "infoBackground": "#33444c",
    "infoFontColor": "#fff",
    "searchbarBackground": "#295b71"
  },
  "projection": {
    "columns": 7
  },
  "detail": {
    "structure": [
      {
        "name": "Title",
        "source": "_title",
        "display": "column",
        "type": "text"
      },
      {
        "name": "Datum",
        "source": "column._day + '. ' + column._month + '. ' + column._year",
        "display": "column",
        "type": "function"
      }
    ]
  }
}
